{
  "name": "miningo",
  "version": "1.0.7",
  "description": "Tiny embedding document-oriented database written in typescript.",
  "main": "index.js",
  "scripts": {
    "dev": "webpack --watch",
    "build": "NODE_ENV=production webpack; tsc; rm -rf ./tmp; npm run build:copy",
    "build:copy": "cp ./package.json ./dist/package.json; cp ./package-lock.json ./dist/package-lock.json; cp ./README.md ./dist/README.md; cp ./LICENSE ./dist/LICENSE",
    "release": "npm run build; cd ./dist; npm publish",
    "test": "jest"
  },
  "typings": "./index.d.ts",
  "author": "RyosukeCla",
  "keywords": [
    "database",
    "embedding",
    "nodejs",
    "browser",
    "typescript",
    "jsonschema"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/RyosukeCla/miningo.git"
  },
  "bugs": {
    "url": "https://github.com/RyosukeCla/miningo/issues"
  },
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "7.1.2",
    "@babel/core": "7.1.2",
    "@babel/plugin-proposal-object-rest-spread": "7.0.0",
    "@babel/plugin-transform-runtime": "7.1.0",
    "@babel/preset-env": "7.1.0",
    "@babel/preset-typescript": "7.1.0",
    "@babel/runtime": "7.1.5",
    "@types/jest": "23.3.9",
    "@types/node": "10.12.3",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "23.6.0",
    "babel-loader": "8.0.4",
    "jest": "23.6.0",
    "ts-jest": "23.10.4",
    "ts-loader": "5.3.0",
    "typescript": "3.1.6",
    "webpack": "4.25.1",
    "webpack-cli": "3.1.2",
    "webpack-node-externals": "1.7.2"
  },
  "dependencies": {
    "jsonschema": "1.2.4"
  }
}
